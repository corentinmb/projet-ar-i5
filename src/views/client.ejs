<html>
	<head>
			<% include css.ejs %>
			<link type="text/css" rel="stylesheet" href="/custom.css"  media="screen,projection"/>  
			<% include js.ejs %>	
	</head>
	<body>
		<div id="message"></div>
  <a-scene embedded arjs='sourceType: webcam; debugUIEnabled: true; trackingMethod: best;' stats>
    <a-marker id="kevin" type='pattern' url='/kevin/kevin.patt'>
			<!--<a-entity obj-model="obj: url(surprise.obj); mtl: url(surprise.mtl);" scale="0.025 0.025 0.025" rotation="-90 0 0"></a-entity>-->
			<a-image src="/kevin/kevin.png" scale="2 2 2" rotation="0 0 0"></a-image>
			<!--<a-entity ply-model="src: url(bulbi.ply);" scale="0.5 0.5 0.5"></a-entity>-->
		</a-marker>
		<a-marker id="val" type='pattern' url='/val/val.patt'>
			<!--<a-entity obj-model="obj: url(surprise.obj); mtl: url(surprise.mtl);" scale="0.025 0.025 0.025" rotation="-90 0 0"></a-entity>-->
			<a-image src="/val/val.png" scale="2 2 2" rotation="0 0 0"></a-image>
			<!--<a-entity ply-model="src: url(bulbi.ply);" scale="0.5 0.5 0.5"></a-entity>-->
	</a-marker>
	<a-marker id="key" type='pattern' url='/key/key.patt'>
		<a-entity obj-model="obj: url(key/key.obj); mtl: url(key/key.mtl);" scale="0.01 0.01 0.01" rotation="-90 0 0"></a-entity>
	</a-marker>
    <!-- define a simple camera -->
    <a-entity camera>
				<a-cursor geometry="primitive: ring" material="color: #464646;    shader: flat"></a-cursor>				
		</a-entity>
		</a-scene>
		<% include ../js/client-script.ejs %>  
	</body>
</html>